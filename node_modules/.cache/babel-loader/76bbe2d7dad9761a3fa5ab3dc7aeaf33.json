AAA,GAAA;AACA,OAPA;AAQA;AAjBA,GARA;AA2BA,EAAA,OA3BA,qBA2BA;AACA,SAAA,WAAA;AACA;AA7BA,CAAA","sourcesContent":["<template>\n  <div class=\"post\" v-if=\"post && author\">\n    <article>\n      <div class=\"image\">\n        <img :alt=\"docum.fields.title\" :src=\"`${post.fields.image[0].fields.file.url}?w=1680&h=750&fit=crop`\">\n      </div>\n      <div class=\"content-wrapper\">\n        <h2>{{ post.fields.title }}</h2>\n        <div class=\"content\" v-html=\"post.fields.content.html\"></div>\n        <div class=\"author\">\n          <img :src=\"`${author.fields.avatar[0].fields.file.url}?w=30&h=30&fit=crop`\" alt=\"author.fields.name\"> Written by {{ author.fields.name }}\n        </div>\n      </div>\n    </article>\n  </div>\n</template>\n\n<script>\n  import Comfortable from 'comfortable-javascript';\n  import { comfortable } from '@/comfortable.js'\n  import _ from 'lodash';\n\n  export default {\n    name: 'document',\n    data() {\n      return {\n        document: null,\n        author: null,\n      }\n    },\n    methods: {\n      getPost() {\n        const options = {\n          embedAssets: true,\n          includes: 1,\n          filters: new Comfortable.Filter()\n            .addAnd('slug', 'equal', this.$route.params.slug)\n        };\n\n        comfortable.getDocuments(options)\n        .then(result => {\n          this.document = result.data[0];\n          this.author = _.find(result.includes.author, { meta: { id: this.document.fields.author[0].meta.id } });\n        })\n        .catch(err => {\n          throw err;\n        })\n      }\n    },\n    created() {\n      this.getDocument();\n    }\n  }\n</script>\n\n<style>\n  .post .image{\n    width: 100%;\n  }\n\n  .post .image img{\n    width: 100%;\n  }\n\n  .post .author img {\n    margin-top: 10px;\n  }\n\n  .post .author img {\n    display: inline-block;\n    margin-bottom: -8px;\n    margin-right: 10px;\n    border-radius: 50%;\n  }\n</style>"],"sourceRoot":"src/views"}]}